/*
 * Device Tree Source for CMT Cube RK3288
 */

/dts-v1/;
#include "rk3288.dtsi"

/ {
	model = "CMT Cube RK3288";
	compatible = "rockchip,rk3288";

	chosen {
		stdout-path = "serial2:115200n8";
	};

	aliases {
		serial2 = &uart2;
	};
};

&uart2 {
	status = "okay";
};

&i2c1 {
	status = "okay";

	rk808: pmic@1b {
		compatible = "rockchip,rk808";
		reg = <0x1b>;
		status = "okay";
	};
};

&sdmmc {
	bus-width = <8>;
	status = "okay";
};

&sdio0 {
	bus-width = <4>;
	status = "okay";
};

&pwm0 {
	status = "okay";
};
